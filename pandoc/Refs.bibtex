pmid = {}, 
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=retrieve&db=pubmed&list_uids=16178059&dopt=Abstract}, 
location = {}, 
keywords = {end-stopping; inhibition; excitation; V1}}


