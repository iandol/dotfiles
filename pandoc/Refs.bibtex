@Article{Jones2002,
author = {Jones, HE and Wang, W and Sillito, AM}, 
title = {Spatial Organization and Magnitude of Orientation Contrast Interactions in Primate V1}, 
journal = {J. Neurophysiol.}, 
volume = {88}, 
number = {5}, 
pages = {2796–2808}, 
year = {2002}, 
abstract = {We have explored the spatial organization of orientation contrast effects in primate V1. Our stimuli were either concentric patches of drifting grating of varying orientation and diameter or grating patches displaced in x-y coordinates around a central patch overlying the classical receptive field (CRF). All cells in the sample exhibited response suppression to iso-oriented stimuli exceeding the CRF. Changing the outer stimulus orientation revealed five response patterns: 1) orientation alignment suppression (17% of cells)-a suppressive component tuned to the same orientation as the cell’s optimal, 2) orientation contrast facilitation (63%)-responses to orientation contrast stimuli exceeded those to the center stimulus alone, 3) nonorientation specific suppression (3%), 4) mixed general suppression and alignment suppression (14%), and 5) orientation contrast suppression (14%)-cross-oriented stimuli evoked stronger suppression than iso-oriented stimuli. Thus most cells (94%) showed larger responses to orientation contrast stimuli than to iso-oriented stimuli, and over one-half showed orientation contrast facilitation. There appeared to be a spatially structured organization of the zones driving the different response patterns with respect to the CRF. Nonorientation-specific suppression and orientation contrast suppression were predominantly evoked by orientation contrast borders located within the CRF, and orientation contrast facilitation was mainly driven by surround stimuli lying outside the CRF. This led to different response patterns according to border location. Zones driving orientation contrast facilitation were not necessarily contiguous to, nor uniformly distributed around, the CRF. Our data support two processes underlying orientation contrast enhancement effects: a simple variation in the strength of surround suppression drawing on the fact that surround suppression is tuned to the same orientation as the CRF and a second process driven by orientation contrast that enhanced cells’ responses to CRF stimulation by either dis-inhibition or orientation contrast facilitation. We suggest these processes may serve to enhance response levels to salient image features such as junctions and corners and may contribute to orientation pop-out.}, 
doi = {10.1152/jn.00403.2001}, 
pmid = {12424313}, 
url = {}, 
location = {}, 
keywords = {}}


@Proceedings{Jones2005,
author = {Jones, H.E. and Wang, Wei and Andolina, IM and Sillito, A.M.}, 
editor = {}, 
title = {Spatial coordination of feedback effects between MT and V1}, 
booktitle = {Spatial coordination of feedback effects between MT and V1}, 
volume = {#854.8}, 
publisher = {}, 
address = {Washington DC}, 
pages = {}, 
year = {2005}, 
abstract = {There is widespread recognition that the interaction between MT and V1 is complex and that processing in V1 can be strongly influenced by feedback from MT. It is also emerging that feedback from MT can influence, via layer 6 of V1, the responses of LGN relay cells that provide the input to V1. Whilst the global nature of these effects are recognized, there is little information on the spatial organization of the influence fed back from a focus in MT to V1 and then in turn to the LGN. We have attempted to provide an insight into this issue by focal pharmacological manipulation of visual response magnitude in area MT whilst assessing the way the location of a discrete visual stimulus with respect to the monitored MT field, influences the feedback effects on cells in V1 and the LGN. 
These results draw on analysis of 35 MT cells paired with 86 cells in V1 and 55 cells in the LGN. In both V1 and the LGN the feedback can increase or decrease the visual response magnitude of cells. In V1 there is strong correlation of effect with receptive field type. At both levels in the system we observed effects at spatial locations ahead of and behind the stimulus focus in MT, suggesting that the feedback exerted a coherent and potentially predictive influence on the way the visual information is processed at earlier levels in the system. We compared this for effects on visual response magnitude and resting discharge where possible. Our data underline the view that a spatially discrete moving stimulus driving MT exerts widespread effects on the processing of visual information in the retinotopic matrices of V1 and the LGN that have the capacity to align network behaviour to the global implications of the stimulus and the way information is processed for a range of parallel and ongoing attributes of the stimulus.}, 
keywords = {}}

@Proceedings{Jones2007,
author = {Jones, H.E. and Andolina, IM and Wang, Wei and Salt, TE and Cudeiro, J and Grieve, KL and Sillito, A.M.}, 
editor = {}, 
title = {Functional organization of MT feedback on LGN processing}, 
booktitle = {Functional organization of MT feedback on LGN processing}, 
volume = {#280.11}, 
publisher = {}, 
address = {}, 
pages = {}, 
year = {2007}, 
abstract = {MT provides feedback to V1 that includes the upper and lower parts of layer 6 which in turn provide feedback to the LGN. This suggests the potential for a fast feedback loop from MT to LGN. We have previously shown that MT does indeed influence the response properties of LGN cells. Here we report results from the effects of focally increasing or decreasing response magnitude in an MT direction column on LGN cell properties. The results from 104 cells suggest a structured pattern of influence that links to the proximity of the LGN receptive fields to the MT field, the vector angle of the location of the LGN cell field with respect to the centre of the MT cell receptive field and its preferred direction, and the direction of stimulus motion.
Many LGN cells showed a notable shift in the locus of their response when the activity of an MT direction column was enhanced. This was linked to the relative proximity of LGN and MT cell receptive fields with a significantly greater proportion of non-overlapping cells showing shifts in their response foci than overlapping cells (p<0.05, Chi-squared test). It was also linked to the relationship between the MT cells’ preferred direction of motion and the angular vector joining the MT and LGN cell receptive fields. There was a significant difference (p<0.05, Kolmogorov-Smirnov two sample test) in the distribution of the angular difference between these two vectors for the cell groups that showed and did not show shifts in response foci. In essence, we found that cells where the angular link was approximately parallel to the MT cell’s preferred direction were most likely to show a shift in angular vector, whilst conversely those that were orthogonal were most likely to show no shift in angular vector. Over 80% of the cells tested showed a significant change in the balance of their responses to a stimulus drifting in the MT columns preferred and non preferred directions when the response magnitude in the MT column was increased or decreased. These emergent asymmetries in LGN cells directional responses indicate that their normal responses to moving stimuli are heavily structured by the feedback originating from MT.}, 
keywords = {}}

