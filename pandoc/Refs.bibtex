@Proceedings{Andolina2010,
author = {Andolina, IM and Jones, H.E. and Ahmed, Bashir and Shipp, S and Clements, J. T. and Copeland, C. S. and Grieve, K.L. and Cudeiro, J and Salt, T. E. and Sillito, A.M.}, 
editor = {}, 
title = {Feedback modulation of spatial summation in primate LGN cells}, 
booktitle = {Feedback modulation of spatial summation in primate LGN cells}, 
volume = {#276.6/PP11}, 
publisher = {}, 
address = {San Diego, CA}, 
pages = {}, 
year = {2010}, 
abstract = {The feedback from layer 6 of V1 to the magno and parvocellular LGN in primates provides a substrate for the rapid modulation of their response properties (Sillito et al. 2006, Briggs and Usrey 2007) and a means for the reflected influence of the cortical processing mechanism to engage the flow of visual information from the retina. In the feline visual system it has been suggested that it enhances center-surround interactions driven by moving stimuli (Murphy and Sillito 1987, Sillito et al. 2006) but it is not clear that this applies in the primate visual system (Sceniak et al. 2006, Alitto and Usrey 2008). We have used pharmacological techniques to probe the influence of the feedback from layer 6 on spatial summation in parvocellular LGN cells. Firstly, we observed the effect of using focal iontophoretic application of GABA to block activity of layer 6 cells in V1 at retinotopic locations overlying the receptive fields of simultaneously recorded parvocellular LGN cells. This changed the way parvocellular LGN cells responded to patches of drifting grating of varying diameter. In particular for some cells it influenced the response to stimuli that fell within the spatial extent of the center mechanism. Thus these responses were reduced when the overlying layer 6 cells were blocked and enhanced when the blockade was removed. This appeared to be an in-line facilitation of the center response to moving stimuli driven by layer 6. The feedback terminals from layer 6 cells in V1 to the LGN are uniquely associated with mGluR1 receptors and so one might predict that a component of their effect is mediated by mGluR1 receptors. Secondly, we have observed that by using iontophoretic techniques to change the level of mGluR1 receptor activation in the LGN we can mimic some of the effects of the feedback on the visual responses of parvocellular LGN cells. This observation is consistent with a previous report in the feline visual system (Rivadulla et al. 2002). Thus for small moving stimuli, the primary effect of the feedback from layer 6 seems to be linked to a focal in-line facilitation of LGN visual responses. This suggests the capacity for a very discrete modulation of focal activity by the feedback.}, 
keywords = {}}

@Proceedings{Andolina2011,
author = {Andolina, IM and Jones, H.E. and Wang, Wei and Sillito, A.M.}, 
editor = {}, 
title = {Inactivation of corticothalamic feedback substantially changes spatial structure of LGN receptive fields}, 
booktitle = {Inactivation of corticothalamic feedback substantially changes spatial structure of LGN receptive fields}, 
volume = {#269.10/HH19}, 
publisher = {}, 
address = {Washington DC}, 
pages = {}, 
year = {2011}, 
abstract = {Relay cells in cat LGN are contacted by single or few afferent retinal inputs, and therefore broadly share similar RF spatial dimensions. Response changes that occur at the level of the visual thalamus are largely described in terms of a modulation in both spatial and temporal domains; feedback from the visual cortex contributes to this transformation (Sillito et al., 2006). Cortical feedback afferents contact thalamic reticular nucleus (TRN) cells, and intrinsic inhibitory interneurons and relay cells. Given the substantial contribution of cortical feedback terminals (direct and indirect), we wanted to finely map RF structure of relay cells on removal of their influence. We utilized local infusion of muscimol, a potent GABAA agonist, in areas 17, 18 and 19 of feline visual cortex and mapped LGN RFs using small spots of light displaced in Cartesian coordinate space. To our surprise we found relay cell RFs responded with excitatory driving over a much larger area of visual space than control RFs. Using areal measurement of significant responses, RF area rose from 0.92°2±0.15 SE (n=29) to 5.31°2±0.92 SE (n=27) when removing feedback; this effect was seen for both X and Y cells. When we isolated responses into burst and tonic components, we found tonic-firing derived area was largely unchanged, but surrounding it was an annular region in which visual stimulation elicited bursts; this annular region contributed significantly to the areal enlargement. There was often a temporal offset between the central tonic and surrounding burst response. A parsimonious hypothesis, given little RGC convergence, is that the enlarged RFs map an extended inhibitory field, in which a rebound hyper-polarizing event drives temporally offset burst responses. Because of the extended spatial configuration, these most probably derive from TRN. In summary, this data maps an extended surround that, though certainly sub-threshold (i.e. modulatory) during normal operation, reveal input over areas of visual space significantly larger than the classically defined RF itself.}, 
keywords = {}}

