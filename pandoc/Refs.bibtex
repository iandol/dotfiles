@Article{Fitzpatrick1994,
author = {Fitzpatrick, D. and Usrey, W. M. and Schofield, B. R. and Einstein, G.}, 
title = {The sublaminar organization of corticogeniculate neurons in layer 6 of macaque striate cortex}, 
journal = {Visual Neuroscience}, 
volume = {11}, 
