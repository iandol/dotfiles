location = {}, 
keywords = {Action Potentials; Action Potentials: physiology; Animals; Macaca mulatta; Macaca mulatta: anatomy & histology; Macaca mulatta: physiology; Models, Neurological; Neurons; Neurons: cytology; Neurons: physiology; Normal Distribution; Pattern Recognition, Visual; Pattern Recognition, Visual: physiology; Photic Stimulation; Psychomotor Performance; Psychomotor Performance: physiology; Reaction Time; Reaction Time: physiology; Saccades; Saccades: physiology; Visual Cortex; Visual Cortex: cytology; Visual Cortex: physiology; Visual Pathways; Visual Pathways: cytology; Visual Pathways: physiology}}


