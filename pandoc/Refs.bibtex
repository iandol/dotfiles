@Article{egger2015,
author = {Egger, R and Schmitt, AC and Wallace, DJ and Sakmann, B and Oberlaender, M and Kerr, JN}, 
title = {Robustness of sensory-evoked excitation is increased by inhibitory inputs to distal apical tuft dendrites.}, 
