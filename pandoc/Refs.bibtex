@Article{Jiang2007,
author = {Jiang, Yi and Zhou, Ke and He, Sheng}, 
title = {Human visual cortex responds to invisible chromatic flicker}, 
journal = {Nat. Neurosci.}, 
volume = {10}, 
number = {5}, 
pages = {657–662}, 
year = {2007}, 
abstract = {When two isoluminant colors alternate at frequencies of 25 Hz or higher, observers perceive only one fused color. Chromatic flicker beyond the fusion frequency induces flicker adaptation in human observers and stimulates monkey V1 neurons. Here we use functional magnetic resonance imaging (fMRI) to show that many human visual cortical areas, with the exception of VO, can distinguish between fused chromatic flicker and its matched nonflickering control. This result supports the existence of significant intracortical temporal filtering of high-frequency chromatic information. The result also suggests that a considerable difference in cortical activation in many visual cortical areas does not necessarily lead to different conscious experiences.}, 
doi = {10.1038/nn1879}, 
