abstract = {Previous work from this laboratory has indicated that postsynaptic mGlu1 receptors are present in the rat thalamus and may contribute to corticothalamic input [1]. The availability of positive allosteric modulators (PAMs) for mGlu1 such as Ro67-4853 [2] offers the possibility of selective potentiation of mGlu1-mediated responses. However, the effect of such potentiation has not been evaluated in vivo. Thus we have evaluated Ro67-4853 effects on responses of ventrobasal thalamus (VB) neurons to sensory stimuli and glutamate receptor agonists.
Extracellular recordings were made in vivo with multi-barrel iontophoretic electrodes from single neurons in the VB of urethane-anaesthetized male adult Wistar rats (205-450g) [3]. Procedures were Home Office (UK) approved and accorded with the Animals (Scientific Procedures) Act 1986. Iontophoretic application of Ro67-4853 was found to potentiate responses to iontophoretic application of the Group I mGlu agonist DHPG (to 264±27% of control responses), consistent with our previous finding that thalamic DHPG responses are mediated primarily via mGlu1 receptors [3]. Furthermore, potentiation of DHPG responses by Ro67-4853 was inversely correlated with the magnitude of the initial responses to DHPG. It is important to note that the PAM did not significantly affect responses to NMDA and that vehicle control ejections had no significant effect on either DHPG or NMDA responses. Sensory responses of VB neurons were evoked by 10-20ms air jets directed at single vibrissae so as to produce a single deflection. Short-latency responses (<50ms) to single vibrissa stimuli were not affected by Ro67-4853, but when repetitive (10Hz) stimulation was maintained for 500-1000ms, responses were potentiated (to 125±6% of control responses) by co-application of Ro67-4853. Such potentiations were prevented by co-application of the mGlu1-selective orthosteric antagonist, LY367385.
These results show that Ro67-4853 can act in vivo as a PAM to potentiate activation of mGlu1 receptors. Furthermore, these findings reveal a contribution of mGlu1 receptors to VB neuron responses to vibrissa stimulation that is recruited upon repetitive stimulation of the sensory pathway and may in part be mediated via corticothalamic feedback.
1. Salt, T.E. Philosophical Transactions of the Royal Society of London Series B 357, 1759-1766 (2002).
2. Knoflach, F. et al. Proceedings of the National Academy of Sciences (USA) 98, 13402-13407 (2001).
3. Salt, T.E. et al. Neuropharmacology 38, 1505-1510 (1999).}, 
keywords = {}}

@Article{Salt2012,
author = {Salt, T. E. and Jones, H.E. and Andolina, IM and Copeland, C. S. and Clements, J. T. and Knoflach, F. and Sillito, A.M.}, 
title = {Potentiation of sensory responses in ventrobasal thalamus in vivo via selective modulation of mGlu1 receptors with a positive allosteric modulator}, 
journal = {Neuropharmacology}, 
volume = {62}, 
number = {4}, 
pages = {1695–1699}, 
year = {2012}, 
abstract = {Metabotropic glutamate subtype 1 (mGlu1) receptor is thought to play a role in synaptic responses in thalamic relay nuclei. The aim of this study was to evaluate the positive allosteric modulator (PAM) Ro67-4853 as a tool to modulate thalamic mGlu1 receptors on single thalamic neurones in vivo. Ro67-4853, applied by iontophoresis onto ventrobasal thalamus neurones of urethane-anaesthetised rats, selectively enhanced responses to the agonist (S)-3,5-dihydroxy-phenylglycine (DHPG), an effect consistent with mGlu1 potentiation. The PAM was also able to enhance maintained responses to 10 Hz trains of sensory stimulation of the vibrissae, but had little effect on responses to single sensory stimuli. Thus Ro67-4853 appears to be a highly selective tool that can be useful in investigating how mGlu1 receptor potentiation can alter neural processing in vivo. Our results show the importance of mGlu1 in sensory processing and attention mechanisms at the thalamic level and suggest that positive modulation of mGlu1 receptors might be a useful mechanism for enhancing cognitive and attentional processes.}, 
doi = {10.1016/j.neuropharm.2011.11.015}, 
pmid = {22178704}, 
url = {http://www.ncbi.nlm.nih.gov/pubmed/22178704}, 
location = {Department of Visual Neuroscience, UCL Institute of Ophthalmology, University College London, 11-43 Bath Street, London EC1V 9EL, United Kingdom. t.salt@ucl.ac.uk}, 
keywords = {Animals; Carbamates/*pharmacology; Male; Neurons/*drug effects/metabolism; Neurons, Afferent/*drug effects/physiology; Rats; Rats, Wistar; Receptors, Metabotropic Glutamate/*metabolism; Thalamus/*drug effects/metabolism; Touch Perception/*drug effects/physiology; Vibrissae/physiology; Xanthenes/*pharmacology}}


@Proceedings{,
author = {Salt, TE and Copeland, C. S. and Jones, h.E. and Andolina, IM and Sillito, A.M.}, 
editor = {}, 
title = {Potentiation of nociceptive responses of thalamic neurones by a metabotropic glutamate receptor (mGlu1) positive allosteric modulator (PAM)}, 
booktitle = {Potentiation of nociceptive responses of thalamic neurones by a metabotropic glutamate receptor (mGlu1) positive allosteric modulator (PAM)}, 
volume = {#614.08/E29}, 
publisher = {}, 
address = {}, 
pages = {}, 
year = {2013}, 
abstract = {We have previously shown that postsynaptic mGlu1 receptors are present in the rat thalamus, and that the mGlu1 PAM Ro67-4853 is able to potentiate such responses in vivo, thus providing a novel means of selectively enhancing mGlu1-mediated synaptic components (Salt etal. 2012). We have now investigated the effect of potentiating mGlu1 receptors on responses of thalamic neurones to noxious sensory stimulation.
Extracellular recordings were made in vivo with multi-barrel iontophoretic electrodes from single neurones in the thalamus of urethane-anaesthetized male adult Wistar rats (205-450g). Procedures were Home Office (UK) approved and accorded with the Animals (Scientific Procedures) Act 1986. In agreement with our previous findings, responses to iontophoretic applications of the Group I mGlu agonist DHPG were selectively potentiated (compared to other glutamate receptor agonists) by co-application of Ro67-4853 to 302±64% of control responses (mean ± s.e.m.) whereas they were selectively reduced upon co-application of the mGlu1 receptor orthosteric antagonist LY367385 to 6±5% of control responses. This indicates that thalamic DHPG responses are mediated primarily via mGlu1 receptors, consistent with the high postsynaptic levels of this receptor in the thalamus. Furthermore, potentiation of DHPG responses by Ro67-4853 appeared to be greater when the initial DHPG response was of a low magnitude. Applications of Ro67-4853 were found to potentiate responses of thalamic neurones to noxious stimulation (52C, applied to either the tail or contralateral hindpaw) to 247±99% of control values (n=6), whilst having little effect on the baseline activity of nociceptive neurones. By contrast, nociceptive responses were reduced to 33±6% of control values (n=9) by concurrent application of LY367385. In a further series of experiments we found that cooling of S1 cortex resulted in a reduction of thalamic nociceptive responses to 14±8% of control values (n=5).
These results underline the importance of mGlu1 receptors in the processing of sensory information in the thalamus, particularly with respect to nociceptive responses. Furthermore, the involvement of mGlu1 receptors may reflect the activity of descending cortico-thalamic afferents from S1.}, 
keywords = {}}

