pmid = {}, 
url = {http://www.ncbi.nlm.nih.gov/sites/entrez?db=pubmed&orig_db=pubmed&term=Spontaneous%20and%20driven%20cortical%20activity%3A%20implications%20for%20computation&cmd=Search&cmd_current=&query_key=11&dopt=AbstractPlus&dispmax=20&sort=&SendTo=&filter_current=&filter=&CrntRpt=abstractplus&showndispmax=20&page=0&dopt1=AbstractPlus&dispmax1=20&sort1=&SendTo1=}, 
