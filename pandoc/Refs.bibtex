keywords = {Action Potentials; Eye; Motion; Movement; TBKW; zPDF}}


@Article{Bura?as2007,
author = {Buračas, GT and Boynton, GM}, 
title = {The effect of spatial attention on contrast response functions in human visual cortex.}, 
journal = {J. Neurosci.}, 
volume = {27}, 
number = {1}, 
pages = {93–97}, 
year = {2007}, 
abstract = {Previous electrophysiology data suggests that the modulation of neuronal firing by spatial attention depends on stimulus contrast, which has been described using either a multiplicative gain or a contrast-gain model. Here we measured the effect of spatial attention on contrast responses in humans using functional MRI. To our surprise, we found that the modulation of blood oxygenation level-dependent (BOLD) responses by spatial attention does not greatly depend on stimulus contrast in visual cortical areas tested [V1, V2, V3, and MT+ (middle temporal area)]. An additive model, rather than a multiplicative or contrast-gain model best describes the attentional modulations in V1. This inconsistency with previous single-unit electrophysiological data has implications for the population-based neuronal source of the BOLD signal.}, 
doi = {10.1016/j.neunet.2012.09.011}, 
pmid = {17202476}, 
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=17202476}, 
