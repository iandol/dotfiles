@Article{Cleland1973a,
author = {Cleland, BG and Levick, WR and Sanderson, KJ}, 
title = {Properties of sustained and transient ganglion cells in the cat retina.}, 
journal = {J. Physiol.}, 
volume = {228}, 
number = {3}, 
pages = {649–680}, 
year = {1973}, 
abstract = {1. The functional basis for a sustained/transient classification of cat retinal ganglion cells has been strengthened by quantitative measurements of the sizes of the centre and surround components of receptive fields. Transient cells had larger surrounds than sustained; the distributions were non-overlapping. Although the distributions of centre sizes overlapped, the transient cells had very significantly larger centres on average.2. There were characteristic differences in area-threshold and annulus-threshold curves and shapes of responses to brief and long flashes of light, and relative differences in the nature of surround adaptation and maintained discharge rates.3. The distinction of sustained from transient units survived over a wide range of backgrounds including the two principle reorganizations of function: relative weakening of surround components at low background; Purkinje shift at high.4. Sustained and transient units were differentially distributed in the retina: there was an overwhelming preponderance of sustained units in the area centralis.5. It is proposed that the transient units are the so-called multidendritedeep cells and the sustained units are the variously styled small ganglion cells.}, 
doi = {10.1113/jphysiol.1973.sp010105}, 
pmid = {4702151}, 
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=4702151}, 
location = {}, 
keywords = {}}


@Article{Cleland1983,
author = {Cleland, B. G. and Lee, B. B. and Vidyasagar, T. R.}, 
title = {Response of neurons in the cat’s lateral geniculate nucleus to moving bars of different length}, 
journal = {J. Neurosci.}, 
volume = {3}, 
number = {1}, 
pages = {108–116}, 
year = {1983}, 
abstract = {It is well recognized that in the visual cortex of the cat, some of the cells (hypercomplex) are sharply tuned for the length of a bar moving backwards and forwards across their receptive fields. Other cells (simple) exhibit no such tuning but appear to respond proportionately over a range of bar lengths. The tuning seen in hypercomplex cells is already observable to a lesser degree in retinal ganglion cells. Our experiments were carried out to determine the extent of this tuning in the lateral geniculate nucleus, which relays information from the retina to the cortex. Results show that geniculate cells have tuning properties intermediate between those of ganglion cells and hypercomplex cells. By adding together a linear array of geniculate cells, it is possible to model the characteristics of a simple cell and to demonstrate that while an elongated bar gives a minimal response in hypercomplex cells, it should have little effect on the response of simple cells.}, 
doi = {}, 
pmid = {6822850}, 
url = {http://www.ncbi.nlm.nih.gov/pubmed/6822850}, 
location = {}, 
keywords = {Animals; Cat; Electric Conductivity; Geniculate Bodies/*physiology; Microelectrodes; Neurons/*physiology; Photic Stimulation; Retina/*physiology; Visual Cortex/*physiology; Visual Fields}}


@Proceedings{,
author = {Clements, JTC and Jones, HE and Andolina, IM and Ahmed, B and Shipp, S and Grieve, KL and Cudeiro, J and Salt, TE and Sillito, AM}, 
editor = {}, 
title = {Differential patterns of spatial summation in primate parvocellular cells assessed from the burst and tonic components of the response to drifting sinusoidal gratings}, 
booktitle = {Differential patterns of spatial summation in primate parvocellular cells assessed from the burst and tonic components of the response to drifting sinusoidal gratings}, 
volume = {#269.11/HH20}, 
publisher = {}, 
address = {Washington DC}, 
pages = {}, 
year = {2011}, 
abstract = {The pattern of spatial summation seen in cat dLGN cells is significantly different when the response is assessed from bursting spikes as opposed to tonic spikes (see Grieve et al poster, this session) and this links to previous work on the significance of burst and tonic firing in vision (Sherman, 2001). Here we have examined the issue in macaque parvocellular LGN cells (n = 78) to see how they compare to the cat. The first noticeable difference is that whilst in the cat approximately 16-8% of the spikes in the response to a drifting sinusoidal grating were burst spikes, in the macaque the percentage was much lower and around 2-3% in accord with previous observations (Ruiz et al, 2006; Alitto et al, 2011). Indeed in our sample only 29 cells exhibited enough reliable burst spikes to enable us to make a valid comparison of tuning properties between burst and tonic firing.
Within this group the optimal diameter for the burst spikes was 0.82° (± 0.09 SE) in comparison to 0.42° (± 0.03 SE) for the tonic spikes. These were significantly different (P < 0.001, Wilcoxon paired rank test). This pattern in the variation of optimal diameter with burst/tonic firing was the same as that observed in the cat LGN cells. However we did not observe a significant difference in the degree of patch suppression seen for the burst and tonic firing. The degree of surround suppression (assessed according to the formula PSI = [1 - (Rplat ÷ Ropt) × 100]) was 75.2 (± 5.65 SE) for the burst firing and 75.4 (± 2.81 SE) for the tonic firing (P > 0.05, Wilcoxon paired rank test). It seems that the contribution of burst firing to macaque parvocellular cell visual responses is much less than for cat LGN cells and whilst where it occurred it was driven by larger optimal diameters than the tonic firing as for the cat, in contrast to the cat there was no difference in response attenuation at larger diameters for burst/tonic firing. The functional significance of this may be that the primate visual system makes less use of the burst firing pattern in processing the visual input and alerting the cortex to the presence of a visual input (cf Sherman, 2001). It also may reflect the fact that there is greater involvement of facilitatory feedback in the responses of primate parvocellular cells to drifting gratings (Andolina et al, 2010; also see Shipp et al poster, this session).}, 
keywords = {}}

@Article{Coen-Cagli2012,
author = {Coen-Cagli, R and Dayan, P and Schwartz, O}, 
title = {Cortical Surround Interactions and Perceptual Salience via Natural Scene Statistics.}, 
journal = {PLoS Comput. Biol.}, 
volume = {8}, 
number = {3}, 
pages = {e1002405}, 
year = {2012}, 
abstract = {Spatial context in images induces perceptual phenomena associated with salience and modulates the responses of neurons in primary visual cortex (V1). However, the computational and ecological principles underlying contextual effects are incompletely understood. We introduce a model of natural images that includes grouping and segmentation of neighboring features based on their joint statistics, and we interpret the firing rates of V1 neurons as performing optimal recognition in this model. We show that this leads to a substantial generalization of divisive normalization, a computation that is ubiquitous in many neural areas and systems. A main novelty in our model is that the influence of the context on a target stimulus is determined by their degree of statistical dependence. We optimized the parameters of the model on natural image patches, and then simulated neural and perceptual responses on stimuli used in classical experiments. The model reproduces some rich and complex response patterns observed in V1, such as the contrast dependence, orientation tuning and spatial asymmetry of surround suppression, while also allowing for surround facilitation under conditions of weak stimulation. It also mimics the perceptual salience produced by simple displays, and leads to readily testable predictions. Our results provide a principled account of orientation-based contextual modulation in early vision and its sensitivity to the homogeneity and spatial arrangement of inputs, and lends statistical support to the theory that V1 computes visual salience.}, 
doi = {10.1371/journal.pcbi.1002405}, 
pmid = {22396635}, 
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=22396635}, 
location = {Dominick Purpura Department of Neuroscience, Albert Einstein College of Medicine, Bronx, New York, United States of America. ruben.coencagli@einstein.yu.edu}, 
keywords = {}}


