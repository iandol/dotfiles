@Proceedings{Jones2010,
author = {Jones, H.E. and Andolina, IM and Ahmed, Bashir and Shipp, S and Clements, J. T. and Grieve, K. and Cudeiro, J and Salt, T. E. and Sillito, A.M.}, 
editor = {}, 
title = {Patch suppression in primate parvocellular LGN cells}, 
booktitle = {Patch suppression in primate parvocellular LGN cells}, 
volume = {#276.3/PP8}, 
publisher = {}, 
address = {San Diego, CA}, 
pages = {}, 
year = {2010}, 
abstract = {Whilst it is recognised that patch suppression characterises the properties of cells in the mammalian LGN, in the macaque, parvocellular cells have been reported to be significantly less patch tuned than magnocellular cells (Alitto and Usrey 2008) with an average patch suppression of 26% compared to 54% in magnocellular cells. This distinction would have important functional implications if true and both these values are considerably less than the average values reported for cortical V1 cells (Jones et al. 2001, Sceniak et al. 2001). We have explored the average degree of patch suppression in parvocellular cells in the macaque using carefully centred patches of drifting sinusoidal grating of varying diameter presented in a randomised interleaved sequence. We find that many parvocellular cells have very strong patch suppression (figure 1) and that the mean degree of patch suppression for parvocellular cells is 80.8% (± 3.68 SE). This is in accord with observations from the study of Sceniak et al. (2006) where they recorded from geniculo-cortical afferents in V1 after cortical inactivation. We suggest that the issue of the patch suppression of primate parvocellular cells needs to be reconsidered and that the evidence points to a high degree of suppression in accord with that characterising primate V1 cells as a whole (Jones et al. 2001, Sceniak et al. 2001). Indeed, the mean degree of patch suppression seen was very similar to that observed across the sample of V1 layer 4C beta cells (78.1% ± 6.96 SE) examined in a previous study of ours (Jones et al. 2001).}, 
keywords = {}}

@Proceedings{Jones2011,
author = {Jones, H.E. and Clements, J. T. and Andolina, IM and Grieve, K. and Cudeiro, J and Shipp, S and Ahmed, Bashir and Salt, T. E. and Sillito, A.M.}, 
editor = {}, 
title = {Temporal evolution of spatial properties in primate parvocellular cells}, 
booktitle = {Temporal evolution of spatial properties in primate parvocellular cells}, 
volume = {#269.14/HH23}, 
publisher = {}, 
address = {Washington DC}, 
pages = {}, 
year = {2011}, 
abstract = {The spatial properties of primate LGN cells derive from the confluence of a series of processes which include a direct excitatory input from retinal ganglion cells, inhibitory input from intrinsic inhibitory interneurons, reflected inhibitory input from TRN cells driven by collaterals of the LGN relay cells and feedback from layer 6 of the visual cortex (Sillito & Kemp, 1983; Sillito et al, 2006; Wang et al, 2006). The feedback from the visual cortex involves direct facilitatory input to the distal dendrites of the relay cells and facilitatory input to the TRN cells. Clearly all these processes will not have the same temporal signature and will be gated in different ways by different classes of visual stimuli. Here we explore the temporal evolution of the spatial properties of primate parvocellular LGN cells (assessed from the tonic response component, see Clements et al poster, this session) to drifting sinusoidal gratings of optimal spatial and temporal frequency.
Several things stand out from the data. The majority of the cells tested (13/21) gave their earliest response to larger than optimal stimuli and the final suppression pattern evolved after this. Only 5 cells showed the earliest response to the cells optimal diameter stimulus with fully developed patch suppression at the same time (cf Alitto & Usrey, 2008). Of particular interest was the fact that the response to the smallest less than optimal stimuli were never seen in the earliest time epochs and evolved later. We highlight this because we observed that these responses to small stimuli were selectively depressed by blockade of corticofugal feedback from cells in layer 6 of V1 with overlying receptive fields to the LGN cells tested. They would seem to depend on a modulatory influence from layer 6 which might be argued to enhance the resolution and focus of the LGN cells.}, 
keywords = {}}

