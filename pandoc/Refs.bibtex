@Proceedings{Shipp2011,
author = {Shipp, S and Jones, H.E. and Andolina, IM and Clements, J. T. and Grieve, K. and Cudeiro, J and Ahmed, Bashir and Salt, T. E. and Sillito, A.M.}, 
editor = {}, 
title = {Spatial extent of V1 feedback influence on cells in primate LGN}, 
booktitle = {Spatial extent of V1 feedback influence on cells in primate LGN}, 
volume = {#269.12/HH21}, 
publisher = {}, 
address = {Washington DC}, 
pages = {}, 
year = {2011}, 
abstract = {We have previously shown by focal GABA inactivation of MT direction columns that feedback from MT modulates the responses of primate LGN cells. These effects were most often seen (although not exclusively) for LGN cells that had receptive fields that did not overlap the extent of the inactivated MT receptive field. The assumption was that these effects are mediated by an influence of the responses of layer 6 cortico-thalamic feedback cells in V1. The implication is that the feedback from a specific location in MT influences an area of layer 6 of V1 that is spatially displaced to (although partially overlapping) the MT receptive field. Another interpretation could be that the feedback influence from macaque V1 to the LGN is biased to locations that are displaced to the layer 6 cell fields. As part of this feedback is mediated via the TRN this could be the case because the connections through this route have not been systematically mapped.
We have explored the effect of focal GABA inactivation of layer 6 of V1 on LGN cell responses at locations overlapping and displaced to the receptive fields of the recorded LGN cells. The effects were clear, all LGN cells with overlapping receptive fields (n=20) showed a change in response magnitude during GABA inactivation of layer 6 of V1. These effects were all reversible and repeatable. For the non-overlapping receptive fields 13/19 cells examined showed changes in response magnitude. The mean separation of the non-overlapping receptive fields was 2.6° (± 0.12 SE) and there was no statistical difference between the separation of those fields showing an effect (n=13, mean separation 2.7° ± 0.12 SE) and those that did not (n=6, mean separation 2.3° ± 0.23 SE). These data show that layer 6 of V1 influences a spatially overlapping area of the LGN, and suggest that the influence extends laterally up to the maximal separation tested at 3.5° away. It underlines the surmise that the influence from MT is likely to be relayed via layer 6 of V1 and indicates that the spatial displacement of the influence from MT to LGN derives from an asymmetry in the organization of the influence of the feedback connections from MT.}, 
keywords = {}}

@Article{Shipp2009,
author = {Shipp, Stewart and Adams, D.L. and Moutoussis, Konstantinos and Zeki, Semir}, 
title = {Feature binding in the feedback layers of area V2}, 
journal = {Cereb. Cortex}, 
volume = {19}, 
number = {10}, 
pages = {2230–2239}, 
year = {2009}, 
abstract = {The visual features of an object are processed by multiple, functionally specialized areas of cerebral cortex. When several objects are seen simultaneously, what mechanism preserves the association of features that belong to a single item? We address this question-known as the “binding problem”-by examining combinatorial feature selectivity of neurons in area V2. In recording from anesthetized macaques, we estimate that dual selectivity for chromatic and spatiotemporal attributes is 50% more common (27% vs. 18% sampling frequency) in superficial and deep layer neurons receiving feedback connections from higher areas, compared with layer 4-3 neurons relaying ascending signals. The operation of feedback pathways is thought to mediate attentional modulation of activity that may achieve binding through acting to select one single object for higher representation and filtering out competing objects. We propose that dual-selective neurons perform a “bridging” function, mediating the transfer of feedback-induced bias between feature dimensions. The bias can be propagated through V2 output neurons (of unitary selectivity) to higher levels of specialized processing and so promote selection of the target object’s representation among multiple feature maps. The bridging function would thus act to unify the outcome of parallel, object-selective processes taking place along segregated visual pathways.}, 
doi = {10.1093/cercor/bhn243}, 
pmid = {}, 
url = {http://www.ncbi.nlm.nih.gov/pubmed/19153106?dopt=Abstract}, 
location = {}, 
keywords = {feedback; binding; V2}}


@Article{Shmiel2005,
author = {Shmiel, Tomer and Drori, Rotem and Shmiel, Oren and Ben-Shaul, Yoram and Nadasdy, Zoltan and Shemesh, Moshe and Teicher, Mina and Abeles, Moshe}, 
title = {Neurons of the cerebral cortex exhibit precise interspike timing in correspondence to behavior}, 
