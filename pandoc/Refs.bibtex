@Article{Sun2007,
author = {Sun, Pei and Ueno, Kenichi and Waggoner, R. and Gardner, Justin and Tanaka, Keiji and Cheng, Kang}, 
title = {A temporal frequency-dependent functional architecture in human V1 revealed by high-resolution fMRI}, 
journal = {Nat. Neurosci.}, 
