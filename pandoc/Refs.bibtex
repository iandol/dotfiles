@Article{Suyatin2013,
author = {Suyatin, Dmitry and Wallman, Lars and Thelin, Jonas and Prinz, Christelle and Jörntell, Henrik and Samuelson, Lars and Montelius, Lars and Schouenborg, Jens}, 
