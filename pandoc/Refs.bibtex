@Proceedings{,
author = {Grieve, K. and Jones, H.E. and Andolina, IM and Clements, JTC and Wang, W and Cudeiro, J and Sillito, A.M.}, 
editor = {}, 
title = {Differential patterns of spatial summation in cat dLGN cells assessed from the burst and tonic components of the response to drifting sinusoidal gratings}, 
booktitle = {Differential patterns of spatial summation in cat dLGN cells assessed from the burst and tonic components of the response to drifting sinusoidal gratings}, 
volume = {#269.13/HH22}, 
publisher = {}, 
address = {Washington DC}, 
pages = {}, 
year = {2011}, 
abstract = {The classic spatial summation curve seen in cat dLGN cells involves a response that increases with increasing diameter up to a point that delivers an “optimal” response and then decreases as the diameter is further increased. The latter reflects what is often referred to as a “surround suppressive” mechanism but in fact the totality of the spatial summation behavior is classically modeled by overlapping excitatory and inhibitory gaussians in the “DOG” model (Rodieck, 1965). In reality the situation is complex with the basic receptive field center-surround characteristics set up by retinal mechanisms, but modified by the pattern of convergence of retinal afferents on LGN relay cells, inhibitory mechanism flowing from local intrinsic inhibitory interneurons, reflected inhibition from interneurons in the TRN and cortical feedback providing direct facilitatory modulatory input to relay cells and indirect modulation of inhibitory effects mediated via intrinsic inteneurons and TRN cells (Sillito & Kemp, 1983; Sillito et al, 2006; Wang et al, 2006). The role of burst versus tonic firing in the LGN and the type of visual information that may be relayed by these different firing patterns has been extensively examined elsewhere (Alitto et al, 2005; Rivadulla et al, 2003; Sherman, 2001). Here we have examined the spatial summation curves of cat dLGN cells as assessed solely from the burst and tonic spikes within their responses to drifting gratings.
Across our sample as a whole (n=48) the optimal diameter assessed from the burst firing of the cells was 1.26° (±0.13 SE) whilst that from the tonic firing was 0.91° (±0.05 SE). These two means are significantly different (P < 0.01, Wilcoxon paired rank test). The patch suppression index (PSI = [1 − (Rplat ÷ Ropt)×100]) was also different with that for the summation curve for burst spikes being 59.5% (±4.35 SE) and for tonic spikes 77.4% (±2.85 SE). Again these were significantly different (P < 0.001, Wilcoxon paired rank test). There appeared to be no difference between the data from X and Y cells; both showed the same pattern of effects. The smaller optimal diameter and stronger surround suppression of the tonic firing means it provides a spatially precise signal to the cortex whilst the burst input provides less precision but may gain more rapid access to the cortical mechanism (Sherman, 2001). The balance in the prevalence of these two mechanisms in the firing pattern may be strongly influenced by the level of corticofugal feedback and the pattern of visual stimulation in the behaving animal.}, 
keywords = {}}

@Article{Grill-Spector2006,
